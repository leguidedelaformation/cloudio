        <input type="radio" style="margin:0 10px 0 20px" {{ block('widget_attributes') }}{% if value is defined %} value="{{ value }}"{% endif %}{% if checked %} checked="checked"{% endif %}/>
    {% endspaceless %}
{% endblock radio_widget %}