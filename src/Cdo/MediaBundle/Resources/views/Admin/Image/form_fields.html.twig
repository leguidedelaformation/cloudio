{% if form.file is defined %}
    <div class="form-group control-label">
        <div class="col-sm-3">
            {{ form_label(form.file) }}
        </div><!-- /.col-sm-3 -->
        <div class="col-sm-9">
            {{ form_widget(form.file) }}
            {{ form_errors(form.file) }}
        </div><!-- /.col-sm-9 -->
    </div><!-- /.form-group -->
{% endif %}
<div class="form-group control-label">
    <div class="col-sm-3">
        {{ form_label(form.alt) }}
    </div><!-- /.col-sm-3 -->
    <div class="col-sm-9">
        {{ form_widget(form.alt) }}
        {{ form_errors(form.alt) }}
    </div><!-- /.col-sm-9 -->
</div><!-- /.form-group -->
<div class="form-group control-label">
    <div class="col-sm-3">
        {{ form_label(form.navbarlogo) }}
    </div><!-- /.col-sm-3 -->
    <div class="col-sm-9 text-left">
        {{ form_widget(form.navbarlogo) }}
        {{ form_errors(form.navbarlogo) }}
    </div><!-- /.col-sm-9 -->
</div><!-- /.form-group -->